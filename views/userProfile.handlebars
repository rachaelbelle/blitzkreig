<div id="profile col-s12">
  <div class="container userProfile">
    <div class="row">
      <div class="col-s12">
        <h2 class="marker text-center"> Blitzkreig! Seige the Day! <img src="/images/tankProfileImg.png" alt="profile"
            class="profile"></h2>
        {{!-- <h3>Welcome <span class="member-name"></span></h3> --}}
        <h3>Welcome {{userName}}</span></h3>


        {{log userName}}
        {{log weather}}
        {{log traffic}}
        {{log news}}
        {{log quotes}}
        {{log zipCode}}

        <div class="col s9 offset-s3">
          {{#if weather}}
          {{> widgets/weather-block zipcZode=zipCode}}
          {{/if}}

        </div>

        <div class="col s9 offset-s3">
          {{#if traffic}}
          {{> widgets/traffic-block zipCode=zipCode}}
          {{/if}}
        </div>

        <div class="col s9 offset-s3">
          {{#if news}}
          {{> widgets/news-block country=zipCode}}
          {{/if}}
        </div>

        <div class="col s9 offset-s3">
          {{#if quotes}}
          {{> widgets/quote-block quotes="Best quote of the day"}}
          {{/if}}
        </div>


      </div>
    </div>
  </div>
</div>

<script src="/js/userProfile.js"></script>