<div id="profile">
  <div class="container">
    <div class="row">
      <div class="col-s12">
        <h1 class="marker">Blitzkreig!   <img src="/images/150.png" alt="profile" class="profile"></h1>

      {{log dataValues}}
      {{log weather}}
      {{log dataValues.weather}}

        {{log this}}

        <div>
          {{#if weather }}
          {{> weather-block}}
          {{/if}}
        </div>

        <div>
          {{#if traffic}}
          {{> traffic-block}}
          {{/if}}
        </div>

        <div>
          {{#if news}}
          {{> news-block}}
          {{/if}}
        </div>

        <div>
          {{#if quotes}}
          {{> quote-block}}
          {{/if}}
        </div>

      </div>
    </div>
  </div>
</div>

<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script src="/js/userProfile.js"></script>
