<div id="profile">
  <div class="container userProfile">
    <div class="row">
      <div class="col-s12">
        <h1 class="marker">Blitzkreig! <img src="/images/tankProfileImg.png" alt="profile" class="profile"></h1>
        <h3>Welcome <span class="member-name"></span></h3>

      {{log dataValues}}
      {{log weather}}
      {{log dataValues.weather}}

        {{log this}}

        <div>
          {{#if weather }}
          {{> weather-block}}
          {{/if}}
        </div>

        <div>
          {{#if traffic}}
          {{> traffic-block}}
          {{/if}}
        </div>

        <div>
          {{#if news}}
          {{> news-block}}
          {{/if}}
        </div>

        <div>
          {{#if quotes}}
          {{> quote-block}}
          {{/if}}
        </div>


      </div>
    </div>
  </div>
</div>

<script src="/js/userProfile.js"></script>
