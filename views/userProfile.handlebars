<div id="profile">
  <div class="container">
    <div class="row">
      <div class="col-s12">
        <h1 class="marker">Blitzkreig!   <img src="/images/150.png" alt="profile" class="profile"></h1>

        <div>
          {{#unless weather}}
          {{> weather-block}}
          {{/unless}}
        </div>

        <div>
          {{#unless traffic}}
          {{> traffic-block}}
          {{/unless}}
        </div>

        <div>
          {{#unless news}}
          {{> news-block}}
          {{/unless}}
        </div>

        <div>
          {{#unless quotes}}
          {{> quote-block}}
          {{/unless}}
        </div>

      </div>
    </div>
  </div>
</div>

<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script src="/js/index.js"></script>
